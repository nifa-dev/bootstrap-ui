{
    "name": "nifa-dev/bootstrap-ui",
    "description": "Twitter Bootstrap 3 support for CakePHP 3 from friendsofcake, modified for AppStack theme",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "twitter", "bootstrap"],
    "homepage": "http://github.com/nifa-dev/bootstrap-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Jared Testa",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/friendsofcake/bootstrap-ui/graphs/contributors"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "support": {
        "issues": "http://github.com/friendsofcake/bootstrap-ui/issues",
        "source": "http://github.com/friendsofcake/bootstrap-ui"
    },
    "autoload": {
        "psr-4": {
            "BootstrapUI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BootstrapUI\\Test\\": "tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
